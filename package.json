{
  "name": "message-queue-examples",
  "version": "1.0.0",
  "description": "Examples of using message queues in Node.js",
  "author": {
    "name": "ExorTek",
    "url": "https://github.com/ExorTek/nodejs-message-queue-examples"
  },
  "keywords": [
    "message-queue",
    "rabbitmq",
    "kafka",
    "sqs",
    "redis",
    "nodejs"
  ],
  "scripts": {
